<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    @*JQuery*@
    <script src="~/scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/scripts/jquery-1.8.0.js"></script>
    @*Font Awesome*@
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" />
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    @*JQuery*@
    <script src="http://code.jquery.com/jquery-1.12.0.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <!--Css Custom-->
    <link href="~/Content/Css/TrangChiTietCss.css" rel="stylesheet" />
    <link href="~/Content/Css/CustomCss.css" rel="stylesheet" />
    <link href="~/Content/Css/GioHangCss.css" rel="stylesheet" />
    <link href="~/Content/Css/CssTrangAdmin.css" rel="stylesheet" />
    @*custom js*@
    <script src="~/scripts/JavaScriptCustom.js"></script>
    <script src="~/scripts/SlideJS.js"></script>
    <script src="~/scripts/jsGiohang.js"></script>
    <script src="~/scripts/DKDNjs.js"></script>

</head>
<body>
    <div class="container-main">
        <header>
            <div id="Welcome">
                <a href="#"><span class="fa fa-hand-peace-o"></span> Xin Chào! @Session["TenAd"]</a>
            </div>
            <div id="Logo">
                <a href="/TrangChu/ViewTrangChu">
                    <img src="~/Content/Images/BookOnline.PNG" />
                </a>
            </div>
            <div id="Search">
                <div id="btn-Search"></div>
                <input type="text" id="txt-Search" placeholder="Tìm Kiếm" name="Search">
            </div>
            <div>
                <img src="~/Content/Images/Admin.PNG" />
            </div>
        </header>
        <div class="Ad-left">
            <h2>Quản Trị Danh Mục</h2>
            <div class="Ad-left1">
                <ul>
                    <li><a href="/Admin/SachAD"><span class="fa fa-chevron-right"></span> Sách</a></li>
                    <li><a href="#"><span class="fa fa-chevron-right"></span> Tác Giả</a></li>
                    <li><a href="#"><span class="fa fa-chevron-right"></span> Chủ Đề Sách</a></li>
                    <li><a href="#"><span class="fa fa-chevron-right"></span> Nhà Xuất bản</a></li>
                </ul>
            </div>
            <h2>Quản Trị Khách hàng</h2>
            <div>
                <ul>
                    <li><a href="#"><span class="fa fa-chevron-right"></span> Khách Hàng</a></li>
                    <li><a href="#"><span class="fa fa-chevron-right"></span> Đơn Hàng</a></li>
                    <li><a href="#"><span class="fa fa-chevron-right"></span> Đánh Giá Của Khách hàng</a></li>
                </ul>
            </div>
        </div>
        <div class="content">
            @RenderBody()
        </div>
    </div>
</body>
</html>
